<!-- app/views/users/_live_search_results.html.erb -->
<ul>
  <% @users.each do |user| %>
    <li>
      <h3><%= user.email %></h3>
      <div class="tags">
        <strong>Skills:</strong>
        <% user.skill_list.each do |skill| %>
          <span class="tag"><%= skill %></span>
        <% end %>
        <br>
        <strong>Expertise of User:</strong>
        <% user.expertise_list.each do |expertise| %>
          <span class="tag"><%= expertise %></span>
        <% end %>
      </div>
    </li>
  <% end %>
</ul>
</div>